<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.netclub.listexample">

    <application
        android:allowBackup="true"
        android:icon="@drawable/forkom_tj"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SimpleListViewActivity" />
        <activity android:name=".activity.CustomListViewActivity" />
        <activity android:name=".activity.RecyclerViewActivity"></activity>
    </application>

</manifest>